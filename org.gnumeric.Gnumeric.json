{
    "id": "org.gnumeric.Gnumeric",
    "runtime": "org.gnome.Platform",
    "runtime-version": "41",
    "sdk": "org.gnome.Sdk",
    "command": "gnumeric",
    "rename-icon": "gnumeric",
    "rename-desktop-file": "gnumeric.desktop",
    "rename-appdata-file": "gnumeric.appdata.xml",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--filesystem=xdg-documents",
        "--talk-name=org.gtk.vfs.*",
        "--filesystem=xdg-run/gvfs",
        "--filesystem=xdg-run/gvfsd"
    ],
    "modules": [
        "shared-modules/intltool/intltool-0.51.json",
        {
            "name" : "gsf",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup" : [
                "/include",
                "/lib/pkgconfig",
                "/share/man",
                "/share/gtk-doc/html/gsf",
                "/share/thumbnailers",
                "/bin/gsf*",
                "*.la"
            ],
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://download.gnome.org/sources/libgsf/1.14/libgsf-1.14.47.tar.xz",
                    "sha256": "d188ebd3787b5375a8fd38ee6f761a2007de5e98fa0cf5623f271daa67ba774d"
                }
            ]
        },
        {
            "name" : "goffice",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup" : [
                "/include",
                "/share/gtk-doc",
                "/lib/pkgconfig",
                "*.la"
            ],
            "sources" : [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/goffice/0.10/goffice-0.10.49.tar.xz",
                    "sha256": "5ffc18dbb385edfb85b6c6254b9e5b4cb3d2ffa3042b932cdbce8e37f4b307e9"
                }
            ]
        },
        {
            "name" : "psiconv",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup" : [
                "/include",
                "/share/man",
                "/share/psiconv",
                "/bin/psiconv*",
                "*.la"
            ],
            "build-options": {
                "cflags": "-fcommon"
            },
            "sources" : [
                {
                    "type": "archive",
                    "url": "http://www.frodo.looijaard.name/system/files/software/psiconv/psiconv-0.9.9.tar.gz",
                    "sha256": "6d51fe79b502a1e277bea275a574ae2db5b1b9d7daef703a8fa3635ae02a8bb0"
                },
                {
                    "type" : "shell",
                    "commands" : [
                        "autoreconf -vfi"
                    ]
                }
            ]
        },
        {
            "name": "pxlib",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup": [
                "/include",
                "*.la"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.sourceforge.net/project/pxlib/pxlib/0.6.8/pxlib-0.6.8.tar.gz",
                    "sha256": "5f9b58a5c3d9e5e60faad16dfbc5172ee14ae57a978732378660c0f2c9e74ef9"
                }
            ]
        },
        {
            "name": "gnumeric",
            "buildsystem": "autotools",
            "cleanup": [
                "/include"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/gnumeric/1.12/gnumeric-1.12.50.tar.xz",
                    "sha256": "758819ba1bd6983829f9e7c6d71a7fa20cd75a3652a271e5bb003d5d8bcc14b8"
                },
                {
                    "type": "patch",
                    "path": "appdata.patch"
                }
            ]
        }
    ]
}
